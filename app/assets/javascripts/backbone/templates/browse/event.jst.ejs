<div class="cover organization">
  <img class="organization-image" src="<%= result.get('thumbnail') %>"/>
  <a href="#organizations/<%= result.get('organization_slug') %>" class="organization-link"><%= result.get('organization') %></a>
</div>
<div class="content">
  <div class="name"><a href="#events/<%= result.get('id') %>"><%= result.get('name') %></a></div>
  <div class="location"><%= result.get('location') %></div>
  <div class="time"><%= startAt %></div>
  <div class="description"><%= result.get('description') %></div>
  <p class="updated-at">Updated <%= moment(result.get('updated_at')).format("M/D H:mm a") %></p>
</div>